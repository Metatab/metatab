{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "list",
            "Action": [
                "s3:ListBucket"
            ],
            "Effect": "Allow",
            "Resource": [
            ],
            "Condition": {
                "StringEquals": {
                    "s3:prefix": [
                        ""
                    ],
                    "s3:delimiter": [
                        "/"
                    ]
                }
            }
        },
        {
            "Sid": "bucket",
            "Effect": "Allow",
            "Action": [
                "s3:GetBucketAcl",
                "s3:GetBucketLocation",
                "s3:ListAllMyBuckets"
            ],
            "Resource": []
        },
        {
            "Sid": "read",
            "Effect": "Allow",
            "Action": [
                "s3:GetObjectAcl",
                "s3:GetObject"
            ],
            "Resource": []
        },
        {
            "Sid": "write",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject"
            ],
            "Resource": []
        }
    ]
}